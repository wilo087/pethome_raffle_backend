generator client {
  provider = "prisma-client-js"
}

datasource postgresql {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model auth {
  id       Int    @default(autoincrement()) @id
  password String
  username String @unique
}

model users {
  code               Int      @unique
  created_at         DateTime @default(now())
  id                 Int      @default(autoincrement()) @id
  identificationCard String
  name               String
  winner             Int      @default(0)
}